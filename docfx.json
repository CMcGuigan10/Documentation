{
    "build": {
        "content": [
            {
                "files": [
                    "en-us/**.md",
                    "en-us/**/toc.yml"
                ],
                "exclude": ["obj/**", "_site/**"]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**",
                    "**/images/**"
                ],
                "exclude": ["obj/**", "_site/**"]
            }
        ],
        "dest": "_site",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": ["templates/reveal"],
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": true,
        "globalMetadata": {
            "_appTitle": "Reveal",
            "_navBarTitle": "Reveal",
            "_isLangEn": true,
            "_navBarTitleHref": "https://www.revealbi.io/",
            "_gitContribute": {
              "repo": "https://github.com/RevealBi/Documentation",
              "branch": "master"
            }
        }
    }
}
